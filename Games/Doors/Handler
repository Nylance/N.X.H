task.wait(0.5)

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Nylance.Xey.Hub", "DarkTheme")

local Tab = Window:NewTab("Home")

local Section = Tab:NewSection("Universal Stuff")

Section:NewButton("Infinite Yield", "Loads IY_FE Admin", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Section:NewButton("NukeVsCity", "Loads NukeVsCity Game Hub - Keyless", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/NukeVsCity/TheALLHACKLoader/main/NukeLoader"))()
end)

Section:NewButton("V.G Hub", "Loads V.G Hub", function()
	local A = loadstring(game:HttpGet("https://raw.githubusercontent.com/1201for/V.G-Hub/main/V.G-Hub-Games-List"))()
	getgenv().Get = setmetatable({},{
		__index = function(A, B)
			return game:GetService(B)
		end}
	)
	for i, v in pairs(Games) do
		if i == game.PlaceId then
			loadstring(game:HttpGet(v))()
		end
	end
	for i, v in pairs(Unknown) do
		loadstring(game:HttpGet(v))()
	end
end)

Section:NewButton("Audio Logger", "Loads a working Audio Logger", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Nylance/XenonCompService/main/Universal/AudioLogger"))()
end)

local Tab2 = Window:NewTab("Doors")

local Section2 = Tab2:NewSection("Doors Stuff")

Section2:NewButton("Vynixius Hub", "Loads Vynixius Hub", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua"))()
end)

Section2:NewButton("Plamen6789 Hub [NORMAL]", "Plamen6789 Hub", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/plamen6789/UtilitiesHub/main/UtilitiesGUI"))()
end)

Section2:NewButton("Plamen6789 Hub [HARD]", "Plamen6789 Hub", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/plamen6789/HardcoreScriptDOORS/main/HardcoreModeScript'))()
end)

Section2:NewButton("Plamen6789 Hub [BIRTHDAY]", "Plamen6789 Hub", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/plamen6789/BirthdayModeDOORS/main/BirthdayModeScript'))()
end)

Section2:NewButton("V.G Hub", "Loads V.G Hub", function()
	local A = loadstring(game:HttpGet("https://raw.githubusercontent.com/1201for/V.G-Hub/main/V.G-Hub-Games-List"))()
	getgenv().Get = setmetatable({},{
		__index = function(A, B)
			return game:GetService(B)
		end}
	)
	for i, v in pairs(Games) do
		if i == game.PlaceId then
			loadstring(game:HttpGet(v))()
		end
	end
	for i, v in pairs(Unknown) do
		loadstring(game:HttpGet(v))()
	end
end)

local Tab3 = Window:NewTab("The Rooms")

local Section3 = Tab3:NewSection("Rooms Stuff")

Section3:NewButton("AutomaticRooms", "Automatically moves you to Door A-1000", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Nylance/XenonCompService/main/Games/Doors/RoomsThing"))()
end)
