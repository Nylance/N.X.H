task.wait(0.2)

ScriptVersion = "0.1.0"

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "NylanceHub Version: "..ScriptVersion.." loaded!",
	Text = "Nylance UtilitiesÂ© 2020 - 2023",
	Duration = 2
})

task.wait(0.1)

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Shadows Scripts HUB V2", "DarkTheme")

local Tab = Window:NewTab("Home")

local Section = Tab:NewSection("Universal Stuff")

Section:NewButton("Infinite Yield", "Loads IY_FE Admin", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Section:NewButton("NukeVsCity", "Loads NukeVsCity Game Hub - Keyless", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/NukeVsCity/TheALLHACKLoader/main/NukeLoader"))()
end)

local Tab2 = Window:NewTab("Player")

local Section2 = Tab2:NewSection("Player Stuff")

Section2:NewSlider("Speed", "Set you speed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section2:NewSlider("Jumppower", "Set you Jumppower", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = s
end)
